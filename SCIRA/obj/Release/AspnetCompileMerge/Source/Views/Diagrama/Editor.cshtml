        if (symbol.id === 'Terminator' || symbol.id === 'Process') {
            symbol.width = 80;
            symbol.height = 40;
        } else if (symbol.addInfo !== undefined) {
            if (symbol.addInfo.id !== undefined) {
                symbol.width = 25;
                symbol.height = 25;
                symbol.annotations = [{ content: symbol.id.substring(1, symbol.id.lenght), annotationType: "string", verticalAlignment: "bottom", offset: { x: 0.5, y: 1 } }];

                //Hacer de solo lectura
                symbol.constraints &= ~(ej.diagrams.NodeConstraints.Resize | ej.diagrams.NodeConstraints.Rotate);
                symbol.constraints |= ej.diagrams.NodeConstraints.ReadOnly;


                if (symbol.shape.shape == "Rectangle") {
                    symbol.style.fill = "green";
                }
                if (symbol.shape.shape == "Triangle") {
                    symbol.style.fill = "red";
                }
            }
        } else {
            symbol.width = 50;
            symbol.height = 50;
        }

        symbol.style.strokeWidth = 2;
        symbol.style.strokeColor = '#757575';
    }

