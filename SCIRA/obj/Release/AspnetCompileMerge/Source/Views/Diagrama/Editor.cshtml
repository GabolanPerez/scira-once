@{
    Layout = "~/Views/Shared/_LayoutIndex.cshtml";
    ViewBag.Title = "DefFunc";
}

<!-- Syncfusion Essential JS 2 Styles -->
<link rel="stylesheet" href="https://cdn.syncfusion.com/ej2/material.css" />

<!-- Syncfusion Essential JS 2 Scripts -->
@*<script src="https://cdn.syncfusion.com/ej2/dist/ej2.min.js"></script>*@
<script src="~/Scripts/ej2/ej2.min.js"></script>

<script>
    //Ocupar todo el espacio de la pantalla
    $(".body-content").removeClass("container");
    $(".body-content").addClass("container-fluid");
    $("body").addClass('loading')
</script>

<h2>Diagrama de @ViewBag.nbRegistro</h2>

<button class="btn btn-default" onclick="window.history.back();">@Strings.getMSG("Regresar")</button>

<br />
<br />

<div class="control-section container">
    <div id="toolbar">
    </div>
    <div id="palette-space" class="sb-mobile-palette">
        @(Html.EJS().SymbolPalette("symbolPalette").Palettes(ViewBag.Palette).ExpandMode(ExpandMode.Multiple).GetNodeDefaults("getNodeDefaults").Width("100%").Height("850px")
                            .SymbolHeight(60).SymbolWidth(60).SymbolMargin(new SymbolPaletteMargin() { Left = 5, Right = 5, Top = 5, Bottom = 5 }).GetSymbolInfo("getSymbolInfo").Render()
                )
    </div>
    <div id="diagram-space" class="sb-mobile-diagram">
        @(Html.EJS().Diagram("container").Width("100%").Height("850px")
                        .SnapSettings(s => s.HorizontalGridlines(ViewBag.gridLinesv).VerticalGridlines(ViewBag.gridLines))
                        .RulerSettings(r => r.ShowRulers(true)
                        .DynamicGrid(false))
                        .DoubleClick("doubleClick")
                        .ContextMenuSettings(c => c.Show(true))
                        .GetConnectorDefaults("getConnectorDefaults").Render()
        )
        @*.ContextMenuClick("contextMenuClick").ContextMenuOpen("contextMenuOpen")*@
    </div>
    <input type="file" id="fileupload" name="UploadFiles" onchange="uploadChange();">
</div>

<!-- #region Formulario -->
@using (Html.BeginForm("Save", "Diagrama", FormMethod.Post, new { id = "frm" }))
{
    @Html.AntiForgeryToken()

    <input type="hidden" id="JsonData" name="JsonData" />
    <input type="hidden" name="id" value="@ViewBag.id" />
    <input type="hidden" name="clave" value="@ViewBag.clave" />
}


<form hidden id="InfoForm">
    <input type="hidden" name="idSP" value="@ViewBag.id" />
    <input type="hidden" id="idReg" name="idReg" />
    <input type="hidden" id="type" name="type" />
</form>


<!-- #endregion -->



