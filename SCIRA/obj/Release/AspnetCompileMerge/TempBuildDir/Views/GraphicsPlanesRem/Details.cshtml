@model IEnumerable<SCIRA.Models.k_plan>
@using SCIRA.Utilidades
<script src="~/Scripts/sharedScripts.js"></script>
<script>
    $(document).ready(function () {
        initTables("#modalB");
    });
</script>

<table class="results table table-striped table-bordered table-hover">
    <thead>
        <tr>
            <th>@Strings.getMSG("GraphicsPlanesRemIndex008")</th>
            <th>@Strings.getMSG("lyPlanesdeRemediacionCreate002")</th>
            <th>@Strings.getMSG("Responsable")</th>
            <th>@Strings.getMSG("Area")</th>
            <th>@Strings.getMSG("GraphicsPlanesRemIndex009")</th>
            <th>@Strings.getMSG("GraphicsPlanesRemIndex010")</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @item.id_plan
                </td>
                <td>
                    @item.ds_plan
                </td>
                <td>
                    @item.c_usuario.nb_usuario
                </td>
                <td>
                    @item.c_area.nb_area
                </td>
                <td>
                    @string.Format("{0:dd/MM/yyyy}", item.fe_estimada_implantacion)
                </td>
                <td>
                    @(item.fe_real_solucion != null ? item.fe_real_solucion.Value.ToString("dd/MM/yyyy"):"N/A")
                </td>
            </tr>
        }
    </tbody>
</table>