



<script>
    $(document).ready(function () {
        var n = @Html.Raw(ViewBag.data);
        jQuery.each(n, function (i, val) {
            chart.removeNode(val);
        });

        n = @Html.Raw(ViewBag.JsonData);
        jQuery.each(n, function (i, val) {
            chart.updateNode(val);
        });
    });
</script>