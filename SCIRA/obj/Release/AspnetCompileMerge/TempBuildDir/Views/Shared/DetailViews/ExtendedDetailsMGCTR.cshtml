<script type="text/javascript">
    function printFile(nombre, id) {
        if (extension(nombre) == "pdf") {

        } else {
            var html = 'No se puede mostrar vista previa.';
            $("#" + id).html(html);
        }
    }

    function extension(filename) {
        return filename.slice((filename.lastIndexOf(".") - 1 >>> 0) + 2);
    }

    $(document).ready(function () {
        printFile("@(Model.c_sub_proceso.nb_archivo_flujo)", "f");
        printFile("@(Model.c_sub_proceso.nb_archivo_manual)", "n");
    });

    $(document).ready(function () {
        if ($("#tiene_accion_correctora").is(':checked')) {
            $("#accion_correctora").html('<a href="#plan" data-toggle="tab" id="accion_correctora_boton"><h4>@Strings.getMSG("RiesgoCreate079")</h4></a>');
            $("#accion_correctora").removeClass("disabled");
            $("#controlTab").html('<a><h4>Control</h4></a>');
            $("#controlTab").addClass("disabled");
            $('#respControl').appendTo('#vacio2');
            $('#AcCr').appendTo('#Contenedor2');
            $('#accion_correctora_boton').trigger('click');
        }
        var encoded;
        var decoded;
        @{
            Type m_tipo = null;
            PropertyInfo[] m_propiedades = null;
            m_tipo = Model.GetType();
            m_propiedades = m_tipo.GetProperties();
            for (int i = 0; i < 20; i++)
            {
                if (ViewBag.CamposExtraControl[i].es_visible && ViewBag.CamposExtraControl[i].aparece_en_mg)
                {
                    var propiedad = m_propiedades[i + 12];
                    string valor = "";
                    try
                    {
                        valor = propiedad.GetValue(Model,null).ToString();
                    }
                    catch
                    {
                        valor = "";
                    }
                    @:encoded = "@valor";
                    @:decoded = $("<div/>").html(encoded).text();
                    @:$("#campo@(String.Format("{0:00}", i+1))").val(decoded);
                    @:$(".campo@(String.Format("{0:00}", i+1))").val(decoded);
                }

            }

            m_tipo = Model.k_riesgo.GetType();
            m_propiedades = m_tipo.GetProperties();

            for (int i = 0; i < 20; i++)
            {
                if (ViewBag.CamposExtraRiesgo[i].es_visible)
                {
                    var propiedad = m_propiedades.Where(p => p.Name == "campo" + string.Format("{0:00}", i + 1)).First();
                    string valor = "";
                    try
                    {
                        valor = propiedad.GetValue(Model.k_riesgo, null).ToString();

                    }
                    catch
                    {
                        valor = "";
                    }
                    @:$("#campor@(String.Format("{0:00}", i+1))").val("@valor");
                }

            }

        }

        $(function () {
            $('#AcCr .toggle').click(function () {
                setTimeout(function () {
                    if ($("#tiene_accion_correctora").is(':checked')) {
                        $("#accion_correctora").html('<a href="#plan" data-toggle="tab" id="accion_correctora_boton"><h4>@Strings.getMSG("RiesgoCreate079")</h4></a>');
                        $("#accion_correctora").removeClass("disabled");
                        $("#controlTab").html('<a><h4>Control</h4></a>')
                        $("#controlTab").addClass("disabled");
                        $('#AcCr').appendTo('#Contenedor2');
                        $('#respControl').appendTo('#vacio2');
                        $('#accion_correctora_boton').trigger('click');
                    } else {
                        $("#accion_correctora").html('<a><h4>@Strings.getMSG("RiesgoCreate079")</h4></a>');
                        $("#accion_correctora").addClass("disabled");
                        $("#controlTab").html('<a href="#control" data-toggle="tab" id="controlTab_boton"><h4>Control</h4></a>')
                        $("#controlTab").removeClass("disabled");
                        $('#AcCr').appendTo('#Contenedor1');
                        $('#respControl').appendTo('#vacio');
                        $('#controlTab_boton').trigger('click');
                    }
                }, 250);
            })
        })

    });
</script>

