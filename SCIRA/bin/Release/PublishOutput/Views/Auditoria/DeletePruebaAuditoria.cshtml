@model SCIRA.Models.c_prueba_auditoria
@using SCIRA.Utilidades
@{
    ViewBag.Title = @Strings.getMSG("Borrar");
    Layout = "~/Views/Shared/_LayoutIndex.cshtml";
}

<h2>@Strings.getMSG("Borrar")</h2>

<h3>@Strings.getMSG("EntidadDelete001")</h3>
<div>
    <h4>@Strings.getMSG("AuditoriaDeletePruebaAuditoria001")</h4>
    <hr />
    <dl class="dl-horizontal">
        <dt>@Strings.getMSG("Nombre")</dt>
        <dd>@Html.DisplayFor(model => model.nb_prueba_auditoria)</dd>

        <dt>@Strings.getMSG("SubProcesoCreate018")</dt>
        <dd>@Html.DisplayFor(model => model.ds_prueba_auditoria)</dd>

        <dt>@Strings.getMSG("NormatividadIndex004")</dt>
        <dd>@Html.DisplayFor(model => model.no_orden)</dd>
    </dl>


    @using (Html.BeginForm())
    {
    @Html.AntiForgeryToken()

    <div class="form-actions no-color">
        <input type="submit" value="@Strings.getMSG("Borrar")" class="btn btn-primary" />
        |
        @Html.ActionLink(Strings.getMSG("Regresar"), "IndexPruebaAuditoria", "Auditoria", new { id = Model.id_auditoria }, new { @class = "btn btn-default btn-sm" })
    </div>
    }
    
</div>
