



<script>
    $(document).ready(function () {
        n = @Html.Raw(ViewBag.JsonData);

        for (var i = 0; i < n.length; i++) {
            var node = n[i];
            switch (node.canEdit) {
                case "0":
                    node.tags = ["noPermission"];
                    break;
            }
        }

        jQuery.each(n, function (i, val) {
            chart.updateNode(val);
        });

        @if(ViewBag.msg != null)
        {
            @:toastr.info('@ViewBag.msg');
        }
    });
</script>