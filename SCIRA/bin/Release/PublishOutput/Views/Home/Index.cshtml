    $(document).ready(function () {
        $('#calendar').calendar({
            dataSource: dataSource,
            mouseOutDay: function(e) {
                if (e.events.length > 0) {
                    $(e.element).find('.card').remove();
                }
            },
            mouseOnDay: function (e) {
                if(e.events.length > 0) {
                    var content = '';
                    content += '<div class="card" style="position:absolute; padding:10px; width: 200px; z-index: 999;">'

                    for(var i in e.events) {
                        content += '<div class="font-weight-bold" style="color:' + e.events[i].color + '">' + e.events[i].name + '</div>'
                                + '<div class="event-location">' + e.events[i].title + '</div>';
                    }

                    content += '</div>'

                    $(e.element).append(content);

                    var coordenadas = $(e.element).find(".card").offset();
                    var alto = $(window).height()
                    var largo = $(window).width()
                    var largoElemento = $(e.element).find(".card").width();

                    if (coordenadas.left + largoElemento + 30 > largo) {
                        $(e.element).find(".card").offset({ left: largo - largoElemento - 30});
                    }
                }
            }
        });
        $("#PrincipalContainer").removeClass('container');
        $("#PrincipalContainer").addClass('container-fluid');
    });

    var toggleD = "0";
    var TCInitialazing = false;

    function toggleDashboard(op) {
        animation.flash();

        var a1 = op;
        var a2 = op == 0 ? 1:0;

        $("#tb"+a2).removeClass('active');
        $("#tb"+a2).addClass('text-white');
        $("#tb"+a1).addClass('active');
        $("#tb"+a1).removeClass('text-white');
        $("#tb"+a2).removeAttr('disabled');
        $("#tb" + a1).attr('disabled', 'disabled');

        setTimeout(function () {
            $("#dsb"+a1).removeAttr('hidden');
            $("#dsb"+a2).attr('hidden', 'hidden');
        }, 350);


        if (op == 1 && !TCInitialazing) {
            TCInitialazing = true;
            $("#dsb1").load("@Url.Action("dsb1","Home")");
        }

    }

</script>

