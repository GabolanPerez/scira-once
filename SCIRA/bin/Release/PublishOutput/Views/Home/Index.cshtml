@using NCrontab
@using SCIRA.Utilidades


@{
    var db = new SCIRA.Models.SICIEntities();

    ViewBag.Title = "Inicio";
    Layout = "~/Views/Shared/_LayoutIndex.cshtml";
    var id = ((SCIRA.Seguridad.IdentityPersonalizado)User.Identity).Id_usuario;

    //Tipos de Fichas
    //  0000 - Recordatorio Simple
    //  0001 - ContenidoNormatividad
    //  0002 - Oficios/Informes
    //  0003 - Incidencias
    //  0004 - Planes


    var user = db.c_usuario.Find(id);
    var fichasUsuario = db.r_evento.Where(e => e.id_responsable == id).ToList();
    var fichasNorm = fichasUsuario.Where(e => e.tipo == "0001").ToList();
    var fichasNormFL = fichasNorm.Where(e => e.recordar_antes_de_vencer);
    var fichasNormR = fichasNorm.Where(e => !e.recordar_antes_de_vencer);

    var fichasOFI = fichasUsuario.Where(e => e.tipo == "0002").ToList();
    var fichasOFIFL = fichasOFI.Where(e => e.recordar_antes_de_vencer);
    var fichasOFIR = fichasOFI.Where(e => !e.recordar_antes_de_vencer);

    var fichasINC = fichasUsuario.Where(e => e.tipo == "0003").ToList();
    var fichasINCFL = fichasINC.Where(e => e.recordar_antes_de_vencer);
    var fichasINCR = fichasINC.Where(e => !e.recordar_antes_de_vencer);

    var fichasPLA = fichasUsuario.Where(e => e.tipo == "0004").ToList();
    var fichasPLAFL = fichasPLA.Where(e => e.recordar_antes_de_vencer);
    var fichasPLAR = fichasPLA.Where(e => !e.recordar_antes_de_vencer);

    var incidencias = user.k_incidencia.Where(inc => inc.id_objeto != null);
    incidencias = incidencias.Where(inc => inc.k_objeto.tipo_objeto == 1 || inc.k_objeto.tipo_objeto == 2 || inc.k_objeto.tipo_objeto == 3);


}
