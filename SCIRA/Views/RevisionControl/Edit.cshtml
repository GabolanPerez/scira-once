<script>
    var nInput = @inputs.Count;
    var InputCount = 0;
    var nPrueba = @pruebas.Count;
    var PruebaCount = 0;
    var $options;


    $("document").ready(function () {
        $options = $("#optionsList > option").sort().clone();
    });


    function addInput() {

        $("#deleteInput").removeAttr("hidden");

        var input = $("<div/>");
        var inputTitle = $("<h4/>");
        var delButton = '<button type="button" class="btn btn-default" onclick="delInput(' + nInput + '); "><i class="fa fa-times" title="@Strings.getMSG("RevisionControlCreate123")"></i></button>';

        input.addClass("row");
        input.attr("id", "I" + nInput);

        inputTitle.addClass("iTitle");

        //nombre
        var nombre = $('<div class="form-group col-md-3"/>');
        var inputName = $('<div class="col-md-12"/>');
        inputName.append('<input type="text" name="nb_input[]" class="form-control" value="" />');

        nombre.append('<label class="control-label col-xs-3 col-md-2">@Strings.getMSG("Nombre")</label>');
        nombre.append(inputName);


        //descripción
        var desc = $('<div class="form-group col-md-3"/>');
        var inputDesc = $('<div class="col-md-12"/>');
        inputDesc.append('<textarea rows="5" name="ds_input[]" class="form-control"></textarea>');

        desc.append('<label class="control-label col-xs-3 col-md-2">@Strings.getMSG("SubProcesoCreate018")</label>');
        desc.append(inputDesc);

        //observaciones
        var observaciones = $('<div class="form-group col-md-3"/>');
        var inputObservaciones = $('<div class="col-md-12"/>');
        inputObservaciones.append('<textarea rows="5" name="obs_input[]" class="form-control"></textarea>');

        observaciones.append('<label class="control-label col-xs-3 col-md-2">@Strings.getMSG("RevisionControlCreate010")</label>');
        observaciones.append(inputObservaciones);

        //trazabilidad
        var traz = $('<div class="form-group col-md-3"/>');
        var inputTraz = $('<div class="col-md-12"/>');
        inputTraz.append('<textarea rows="5" name="tr_input[]" class="form-control"></textarea>');

        traz.append('<label class="control-label col-xs-3 col-md-2">@Strings.getMSG("RevisionControlCreate117")</label>');
        traz.append(inputTraz);

        //calificacion
        var calif = $('<div class="form-group col-md-3"/>');
        var inputCalif = $('<div class="col-md-12"/>');
        var selectCalif = $('<select class="form-control" data-content="@Strings.getMSG("RevisionControlCreate020")" data-toggle="popover" data-trigger="hover"  name="calif_input[]" title="" data-original-title="@Strings.getMSG("Atencion")"/>');
        selectCalif.append($options.clone());
        inputCalif.append(selectCalif);


        calif.append('<label class="control-label col-xs-3 col-md-2">@Strings.getMSG("EvaluacionIndicadorEvaluaciones003")</label>');
        calif.append(inputCalif);




        input.append(inputTitle);
        input.append(delButton);
        input.append(nombre);
        input.append(desc);
        input.append(traz);
        input.append(observaciones);
        input.append(calif);
        input.append('<input name="id_input[]" hidden value="0"/>');


        $("#inputs").append(input);

        nInput++;

        assignInputName();
    }

    function delInput(id) {
        $("#I" + id).remove();

        assignInputName();
    }


    function assignInputName() {
        InputCount = 0;

        $(".iTitle").each(function () {
            InputCount++;
            var $this = $(this);

            $this.text("Input " + InputCount);
        });
    }

    function assignPruebaName() {
        PruebaCount = 0;

        $(".pTitle").each(function () {
            PruebaCount++;
            var $this = $(this);

            $this.text("Prueba " + PruebaCount);
        });
    }


    function addPrueba() {

        $("#deletePrueba").removeAttr("hidden");

        var prueba = $("<div/>");
        var pruebaTitle = $("<h4/>");
        var delButton = '<button type="button" class="btn btn-default" onclick="delPrueba(' + nPrueba + '); "><i class="fa fa-times" title="@Strings.getMSG("RevisionControlCreate123")"></i></button>';

        prueba.addClass("row");
        prueba.attr("id", "P" + nPrueba);

        pruebaTitle.addClass("pTitle");

        //descripción
        var desc = $('<div class="form-group col-md-8"/>');
        var pruebaDesc = $('<div class="col-md-12"/>');
        pruebaDesc.append('<textarea rows="5" name="ds_pp[]" class="form-control"></textarea>');

        desc.append('<label class="control-label col-xs-3 col-md-2">@Strings.getMSG("SubProcesoCreate018")</label>');
        desc.append(pruebaDesc);

        //observaciones
        var observaciones = $('<div class="form-group col-md-4"/>');
        var inputObservaciones = $('<div class="col-md-12"/>');
        inputObservaciones.append('<textarea rows="5" name="obs_pp[]" class="form-control"></textarea>');

        observaciones.append('<label class="control-label col-xs-3 col-md-2">@Strings.getMSG("RevisionControlCreate010")</label>');
        observaciones.append(inputObservaciones);

        //calificacion
        var calif = $('<div class="form-group col-md-4"/>');
        var pruebaCalif = $('<div class="col-md-12"/>');
        var selectCalif = $('<select class="form-control" data-content="@Strings.getMSG("RevisionControlCreate020")" data-toggle="popover" data-trigger="hover"  name="calif_pp[]" title="" data-original-title="@Strings.getMSG("Atencion")"/>');
        selectCalif.append($options.clone());
        pruebaCalif.append(selectCalif);


        calif.append('<label class="control-label col-xs-3 col-md-2">@Strings.getMSG("EvaluacionIndicadorEvaluaciones003")</label>');
        calif.append(pruebaCalif);




        prueba.append(pruebaTitle);
        prueba.append(delButton);
        prueba.append(desc);
        prueba.append(observaciones);
        prueba.append(calif);
        prueba.append('<input name="id_pp[]" hidden value="0"/>');

        $("#pruebas").append(prueba);

        nPrueba++;

        assignPruebaName();
    }

    function delPrueba(id) {
        $("#P" + id).remove();

        assignPruebaName();
    }

    function deleteFile(id,nb_archivo) {
        confirmationModal.initModal({
            title: '@Strings.getMSG("AuditoriaEditSeguimientoObservaciones007")' + nb_archivo + '?',
            action: 'deleteFileConfirm('+id+')'
        });
    }

    function deleteFileConfirm(id) {
        //eliminar archivo con ajax
        $.ajax({
            dataType: "html",
            type: "post",
            url: '@(Url.Action("DeleteByUser","Archivos"))',
            data: { id },
            success: function (data) {
                if (data == id) {
                    toastr.success("@Strings.getMSG("AuditoriaEditSeguimientoObservaciones008")");
                    //eliminar fila
                    $("#fr" + id).remove();
                }
            },
            error: function (xhr, ajaxOptions, thrownError) {
                toastr.error("@Strings.getMSG("AvisoError003")");
            }
        });
    }

    $(function () {
        $('#cg_calificacion_toggle').change(function () {
            var val = $(this).prop('checked');
            if (val) {
                $('#cg_calificacion').val("1");
            } else {
                $('#cg_calificacion').val("0");
            }
        });
    });

    $(function () {
        $('.panel-heading').click(function () {
            var $this = $(this);
            setTimeout(function () {
                var offset2 = $this.offset().top;
                $('html,body').animate({
                    scrollTop: offset2 - 100
                }, 100);
            }, 500);
        });
    });
</script>


