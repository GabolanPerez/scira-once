            $(function () {
                chat.client.refreshNotif = function (id, json) {
                    if (UserID == id) {
                        var NotifData = JSON.parse(json);
                        refreshNotif(NotifData);
                    }
                };

                //Notificaciones emergentes en tiempo real
                chat.client.notifyUser = function (id, message,notifType) {
                    if (UserID == id) {
                        if (notifType == "success") toastr.success(message);
                        if (notifType == "error") toastr.error(message);
                        if (notifType == "warning") toastr.warning(message);
                        if (notifType == "info") toastr.info(message);
                    }
                };

                chat.client.execute = function (id, expression) {
                    if (UserID == id) {
                        eval(expression);
                    }
                }

                //Iniciar conexión
                $.connection.hub.start().done(function () {

