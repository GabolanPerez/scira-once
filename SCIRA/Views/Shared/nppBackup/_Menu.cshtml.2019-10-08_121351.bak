        $(".NotifIndicadoresDiariosTC").html(NotifData.t_indicadores_diariosTC - NotifData.n_indicadores_diariosTC + " de " + NotifData.t_indicadores_diariosTC);
        $(".ProgressBarIndicadoresDiariosTC").css('width', PorcentajeIndicadoresDiariosTC + "%");
    }
</script>

